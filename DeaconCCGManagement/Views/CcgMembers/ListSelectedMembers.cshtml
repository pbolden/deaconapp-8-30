@using DeaconCCGManagement.Helpers
@using DeaconCCGManagement.Models
@using DeaconCCGManagement.ViewModels
@model IList<ListMembersViewModel>


@{
    ViewBag.Title = "ListSelectedMembers";
}

@Html.Partial("_Breadcrumbs", "Selected Members")

<h2>Selected Members</h2>

@if (Model.Count == 0)
{
    <h3>No Members Selected</h3>
}

@{
    int num = 1;
    foreach (var item in Model)
    {
        <label>@num. @item.FullName </label> <br/>
        num++;
    }
}